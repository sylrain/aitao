<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aitao.dao.VersionMapper">
    <select id="query" resultType="com.aitao.domain.po.Version">
        SELECT
        code,
        name,
        url
        FROM tb_version
        ORDER BY code DESC
        LIMIT 0,1
    </select>
</mapper>